(defun std_transform (l fn)
		(set i 0)
		(while (< i (length l))
			(set result (append result (fn (get l i))))
			(set i (+ i 1))
			)
		(return result)
	)

(defun std_pow (x p)
	(set i 0)
	(set s x)
	(set p (- p 1))
	(while (< i p)
		(set s (* s x))
		(set i (+ i 1))
		)
	(return s)
	)

